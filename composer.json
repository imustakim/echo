{
    "name": "imustakim/echo",
    "description": "A lightweight PHP framework",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^9.0",
        "symfony/http-foundation": "^6.0",
        "symfony/routing": "^6.0",
        "monolog/monolog": "^3.0",
        "vlucas/phpdotenv": "^5.0",
        "illuminate/validation": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}